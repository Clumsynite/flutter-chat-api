{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": "off",
    "object-curly-newline": "off",
    "operator-linebreak": "off",
    "no-useless-escape": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": "off"
  }
}
